{
  "name": "translate",
  "displayName": "translate",
  "description": "translate",
  "version": "0.0.1",
  "publisher": "HenryTSZ",
  "homepage": "https://github.com/HenryTSZ/vsc-translate/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/HenryTSZ/vsc-translate.git"
  },
  "bugs": {
    "url": "https://github.com/HenryTSZ/vsc-translate/issues"
  },
  "icon": "icon.png",
  "keywords": [
    "translate",
    "baidu"
  ],
  "activationEvents": [
    "onCommand:translate.replace"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "translate.translate",
        "title": "translate"
      }
    ],
    "keybindings": [
      {
        "command": "translate.translate",
        "key": "Ctrl+Alt+X",
        "mac": "Cmd+Alt+X",
        "when": "editorTextFocus&&editorHasSelection"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "翻译服务配置",
      "properties": {
        "translate.baiduAppid": {
          "type": "string",
          "default": "",
          "description": "百度翻译APP ID (Baidu Translate APP ID)"
        },
        "translate.baiduSecretKey": {
          "type": "string",
          "default": "",
          "description": "百度翻译密钥 (Baidu Translate Secret Key)"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.6.5"
  }
}
